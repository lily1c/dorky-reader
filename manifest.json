{
  "manifest_version": 3,
  "name": "Dorky - Your Reader",
  "version": "1.0",
  "description": "Dorky reads aloud with a natural voice",
  "host_permissions": [
    "*://*.elevenlabs.io/*"
  ],
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["config.js", "scripts.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
